@model List<Alumno>

@{
    ViewData["Title"] = "Info Alumno";
}
 
<strong>Desde la vista parcial</strong>

<!-- @foreach (var alumno in Model)
{
    <partial name="ObjetoEscuelaSimple" model="alumno"/>
} -->

<table class="table table-dark table-hover table-sm">
    <thead>
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col">Id</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var alumno in Model)
        {
            <tr>
                <td>@alumno.Nombre</td>
                <td>@alumno.UniqueId</td>
            </tr>
        }
    </tbody>
</table>


<p>@ViewBag.Fecha</p>
